<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="RuneDocMVVM.App"
             xmlns:local="using:RuneDocMVVM"
             xmlns:sty="using:FluentAvalonia.Styling"
             RequestedThemeVariant="Dark"
             xmlns:dialogHostAvalonia="clr-namespace:DialogHostAvalonia;assembly=DialogHost.Avalonia"
             xmlns:views="clr-namespace:RuneDocMVVM.Views"
             xmlns:viewmodels="clr-namespace:RuneDocMVVM.ViewModels"
             >
    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>
  
    <Application.Styles>
        <sty:FluentAvaloniaTheme PreferUserAccentColor="True" PreferSystemTheme="True"></sty:FluentAvaloniaTheme>
        <StyleInclude Source="avares://RuneDocMVVM/Assets/Icons.axaml"/>
        <dialogHostAvalonia:DialogHostStyles />
    </Application.Styles>
             

</Application>